{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": "3.6.4"
      }
    ],
    [
      "@babel/preset-react",
      {
        "pargma": "dom",
        "pragaFrag": "DomFrag",
        "throwIfNamespace": false
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-block-scoping",
    "@babel/plugin-transform-classes",
    "@babel/plugin-syntax-dynamic-import"
  ]
}
